{
    "name": "tothbarnabas79/cake-excel",
    "type": "cakephp-plugin",
    "description": "CakePHP excel generator",
    "keywords": [
        "cakephp",
        "tothbarnabas79",
        "excel",
        "phpexcel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.6",
        "phpoffice/phpspreadsheet": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeExcel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeExcel\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
